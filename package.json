{
  "name": "@travel-packlist",
  "version": "0.3.3",
  "license": "MIT",
  "scripts": {
    "changelog": "conventional-changelog -i CHANGELOG.md -n util/conventionalcommits.mjs",
    "version": "npm run changelog -- -s && git add CHANGELOG.md",
    "release-notes": "conventional-changelog -r 2 -n util/conventionalcommits.mjs -o",
    "prepare": "husky",
    "format": "nx format --all",
    "dev": "nx serve travel-packlist",
    "start": "nx serve travel-packlist-web"
  },
  "nx": {
    "includedScripts": [
      "format",
      "dev",
      "start"
    ],
    "targets": {
      "lint": {
        "executor": "@nx/eslint:lint",
        "options": {
          "lintFilePatterns": [
            "*.*",
            "util/*.*"
          ]
        }
      },
      "markdownlint": {
        "executor": "@travel-packlist/check-plugin:markdown",
        "options": {
          "pattern": "*.md"
        }
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.3",
    "@angular/common": "^19.0.3",
    "@angular/compiler": "^19.0.3",
    "@angular/compiler-cli": "^19.0.3",
    "@angular/core": "^19.0.3",
    "@angular/forms": "^19.0.3",
    "@angular/localize": "19.0.4",
    "@angular/platform-browser": "^19.0.3",
    "@angular/platform-browser-dynamic": "^19.0.3",
    "@angular/router": "^19.0.3",
    "@angular/service-worker": "^19.0.3",
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "^6.0.2",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.2.0",
    "@capacitor/core": "^6.2.0",
    "@capacitor/filesystem": "^6.0.2",
    "@capacitor/share": "^6.0.3",
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "20.2.2",
    "@nx/devkit": "20.2.2",
    "@nx/eslint": "20.2.2",
    "@nx/eslint-plugin": "20.2.2",
    "@nx/jest": "20.2.2",
    "@nx/js": "20.2.2",
    "@nx/playwright": "20.2.2",
    "@nx/plugin": "20.2.2",
    "@nx/web": "20.2.2",
    "@nx/workspace": "20.2.2",
    "@playwright/test": "^1.36.0",
    "angular-eslint": "^19.0.2",
    "eslint-plugin-perfectionist": "^4.2.0",
    "eslint-plugin-playwright": "^1.6.2",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "json-schema-to-typescript": "^15.0.3",
    "nx": "20.2.2",
    "peggy": "^4.2.0",
    "rxjs": "~7.8.1",
    "showdown": "^2.1.0",
    "tailwindcss": "^3.4.16",
    "tslib": "^2.8.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "8.17.0"
  },
  "devDependencies": {
    "@cspell/cspell-types": "^8.16.1",
    "@cspell/dict-de-de": "^4.0.3",
    "@nx/storybook": "20.2.2",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-interactions": "^8.4.6",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/addon-viewport": "^8.4.7",
    "@storybook/angular": "^8.4.6",
