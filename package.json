{
  "name": "travel-packlist",
  "version": "0.1.0",
  "scripts": {
    "version": "conventional-changelog -i CHANGELOG.md -n conventionalcommits.mjs -s && git add CHANGELOG.md",
    "release-notes": "conventional-changelog -r 2 -n conventionalcommits.mjs -o",
    "ng": "ng",
    "generate-assets": "node generate-assets.mjs",
    "prestart": "npm run generate-assets -- --pwa",
    "start": "ng serve --host 0.0.0.0",
    "prebuild": "npm run generate-assets -- --pwa",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --ci",
    "prepare": "husky && npm run convert-documentation && npm run generate-icons",
    "convert-documentation": "node convert-documentation.mjs",
    "generate-icons": "node generate-icons.mjs",
    "format": "prettier --write \"**/*.*\"",
    "lint": "ng lint",
    "stylelint": "stylelint \"**/*.css\"",
    "extract-i18n": "ng extract-i18n",
    "postextract-i18n": "npm run check-translations",
    "check-translations": "node check-translations.mjs",
    "spellcheck": "cspell \"**/*.md\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/build": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/localize": "19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/service-worker": "^19.0.0",
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "^6.0.2",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.2.0",
    "@capacitor/core": "^6.2.0",
    "@capacitor/filesystem": "^6.0.2",
    "@capacitor/share": "^6.0.3",
    "husky": "^9.1.7",
    "rxjs": "~7.8.1",
    "showdown": "^2.1.0",
    "tailwindcss": "^3.4.15",
    "tslib": "^2.8.1",
    "typescript": "~5.6.3"
  },
  "devDependencies": {
    "@cspell/cspell-types": "^8.16.0",
    "@cspell/dict-de-de": "^4.0.3",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "angular-eslint": "18.4.1",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cspell": "^8.16.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.3.2",
    "prettier": "^3.3.3",
    "prettier-plugin-sh": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sort-json": "^2.0.1",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript-eslint": "8.15.0",
    "zone.js": "~0.15.0"
  }
}
