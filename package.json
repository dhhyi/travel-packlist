    "@types/jest": "^29.5.14",
    "@types/node": "18.16.9",
    "@typescript-eslint/utils": "^8.13.0",
    "autoprefixer": "^10.4.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cspell": "^8.16.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.0.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.4.2",
